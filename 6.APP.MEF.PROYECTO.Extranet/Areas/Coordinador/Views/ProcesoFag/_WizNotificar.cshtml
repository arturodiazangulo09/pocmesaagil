                                                @if (Model.ARCHIVO_TDR > 0)
                                                {
                                                    <div class="control" id="DivM_Anexo1" style="text-align:center">
                                                        <a title='Descargar archivo' onclick='fn_DescargarArchivo(@Model.ID_ARCHIVO_CONTRATO);' style="font-size: 20px; color:#0093F8; font-weight: bold; cursor: pointer"><i class="fas fa-download"></i> <small style="font-size:13px;font-weight:bold">Descargar archivo</small></a>
