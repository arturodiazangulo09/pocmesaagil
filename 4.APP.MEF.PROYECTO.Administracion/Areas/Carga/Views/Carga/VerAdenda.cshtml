@model MEF.PROYECTO.Entity.CargaMasiva.Cls_Ent_Carga_Cabecera
@{
    ViewBag.Title = "VerAdenda";
}
 <div class="modal-dialog modal-lg">
     <div class="modal-content">
         <div class="modal-header bg-danger">

             <h5 class="modal-title" id="staticBackdropLabel">Adendas Asociadas</h5>
             <button type="button" class="close" id="btnCloseModal" data-dismiss="modal" aria-label="Close">
                 <span aria-hidden="true">&times;</span>
             </button>

         </div>

         <div class="modal-body">
             <div class="card card-default">
                 <div class="card-body">
                     <div class="table-responsive">
                         <table id="tablaAdendas" class="table table-bordered dt-responsive dataTable table-hover" style="width: 100%; white-space: nowrap;">
                         </table>
                         <table class="table" id="tablaAdendas">
                             <thead>
                                 <tr>
                                     <th scope="col">COD CONTRATO</th>
                                     <th scope="col">DOC SOLICITUD</th>
                                     <th scope="col">HOJA RUTA</th>
                                     <th scope="col">NUMERO</th>
                                 </tr>
                             </thead>
                             <tbody id="tablaAdendasBody"></tbody>
                         </table>
                     </div>
                 </div>
             </div>
         </div>

         <div class="modal-footer justify-content-between">
             <button id="btnSalirModal"  name="btnSalirModal" class="btn btn-dark" type="button" data-dismiss="modal">Salir</button>
         </div>
     </div>

    </div>
 <script src="@Url.Content("~/Scripts/js/Carga/CargaMantenimiento.js")" type="text/javascript"></script>
 <script type="text/javascript">
    CargaMantenimiento.ModalIniciar(@(ViewBag.ID_REGISTRO));
 </script>
