<script type="text/javascript">
    //Validando numeros
    $("#TXT_NRO_EXP_SISPER,#TXT_DNI,#TXT_RUC,#TXT_EDAD,#TXT_TELEFONO_CELULAR,#TXT_NIVEL_GOBIERNO,#TXT_ADENDA_VIG_NRO,#TXT_EXP_LAB_GENERAL,#TXT_EXP_LAB_ESPECIFICA,#TXT_EXP_LAB_GENERAL_GRADO,#TXT_EXP_LAB_ACT_TDRS").on('keypress', function (e) {
        DESARROLLO.SOLONUMEROS(e);
    });
    //Validando decimales
    $("#TXT_IMPORTE_HONORARIO").on('keypress', function (e) {
        DESARROLLO.NUMERODECIMAL(e);
    });
    jQuery("#btnGrabar").click(function (e) {
            DESARROLLO.PROCESO_CONFIRM("</br>¿Desea <b>Actualizar</b> el registro seleccionado? </br>", function (r) {
                if (r) {
                    CargaMantenimiento.fn_Mantenimiento_FAG(@(ViewBag.ID_REGISTRO));
                }
            });

    });
</script>
