<script type="text/javascript">
    $(function () {
        CrearGrillaFormacion();
        CargarGrillaFormacion();
        if ($("#ACCION").val() == "F") {
            CrearGrillaEspecializacion();
            CargarGrillaEspecializacion();
        } else {
            CrearGrillaCapacitacion();
            CargarGrillaCapacitacion();
        }
        CrearGrillaExperiencia();
        CargarGrillaExperiencia();
    });
    jQuery('#btnGrabarDocumentos').click(function (e) {
        var Valor = true;
        $(".CALIFICAR").each(function () {
            text = $(this).val();
            if (text == "") {
                Valor = false;
            }
        });
        if (Valor) {
            DESARROLLO.PROCESO_CONFIRM("</br>¿Desea <b>Registrar</b> la calificación de documentos.? </br>", function (r) {
                if (r) {
                    fn_UPDCalificarDcocumento($("#ACCION").val());
                }
            })
        } else {
            DESARROLLO.PROCESO_ALERT("Atención", "Califique correctamente todos los documentos, recuerde que los documentos observados pueden ser enviados al coordinador de la entidad.");

        }

    });
</script>
